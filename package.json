{
  "name": "js-package",
  "description": "cocaine, but for JavaScript open-source packages",
  "version": "0.0.7",
  "author": "Chris Andrejewski <christopher.andrejewski@gmail.com>",
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "bin": {
    "js-package": "bin/js-package.js"
  },
  "bugs": {
    "url": "https://github.com/andrejewski/js-package/issues"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "babel-regenerator-runtime": "^6.5.0",
    "babel-register": "^6.11.6",
    "del": "^2.2.1",
    "fixpack": "^2.3.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-sourcemaps": "^1.6.0",
    "nyc": "^9.0.1",
    "pre-commit": "^1.1.3",
    "pre-push": "^0.1.1",
    "source-map-support": "^0.4.2",
    "standard": "^8.4.0"
  },
  "homepage": "https://github.com/andrejewski/js-package#readme",
  "keywords": [
    "node",
    "npm",
    "package",
    "starter"
  ],
  "license": "ISC",
  "main": "lib/index.js",
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "pre-commit": [
    "lint"
  ],
  "pre-push": [
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrejewski/js-package.git"
  },
  "scripts": {
    "build": "gulp",
    "lint": "standard --fix && fixpack",
    "prepublish": "npm run build && npm run test",
    "report": "NODE_ENV=nyc nyc npm test",
    "test": "npm run lint && ava",
    "watch": "gulp watch"
  }
}
